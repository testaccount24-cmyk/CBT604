         EX    7,S4$00A64                . 000A9C 4470 B056
         MVC   0(8,6),0(1)               . 000AA0 D207 6000 1000
         L     5,0(6)                    . 000AA6 5856 0000
         IC    5,8(4,7)                  . 000AAA 4354 7008
S4$00AAE LR    4,5                       . 000AAE 1845
         SRDL  4,19                      . 000AB0 8C40 0013
