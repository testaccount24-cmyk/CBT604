         B     AUG1                      . 00056E 47F0 B05C
S4$00572 LD    0,A2PTR                   . 000572 6800 A368
         STD   0,8(15)                   . 000576 600F 0008
         LD    0,A3PTR                   . 00057A 6800 A370
         STD   0,16(15)                  . 00057E 600F 0010
